<!DOCTYPE html>
<html>
<head>
  <title><%= A9n.app_name %></title>
  <%= stylesheet_link_tag 'service', media: 'all' %>
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->
  <%= csrf_meta_tags %>
</head>
<body class="skin-green">
  <div class="wrapper">
    <%= render 'layouts/service/navbar' %>
    <%= render 'layouts/service/sidebar' %>
    <div class="content-wrapper">
      <%= render 'layouts/service/content_header' %>
      <section class="content">
        <div class="row">
          <div class="col-md-12">
            <%= render 'layouts/service/flash_message' %>
            <div class="box box-danger">
              <%= yield %>
            </div>
          </div>
        </div>
      </section>
    </div>
  </div>

  <%= javascript_include_tag 'service' %>
  <%= tinymce %>
</body>
</html>
