var userLocation = new google.maps.LatLng('<%= @place_info.latitude %>', '<%= @place_info.longitude %>');

centerTo(userLocation, 16);

$('.x-place-content').html(
  '<%= "<p><strong>Adres:</strong> #{@place_info.data[:info]}</p>".html_safe if @place_info.data[:info] %>' +
  '<%= "<p><strong>Nazwa:</strong> #{@place_info.data[:name]}</p>".html_safe if @place_info.data[:name] %>' +
  '<%= "<p><strong>Kod pocztowy:</strong> #{@place_info.data[:post_code]}</p>".html_safe if @place_info.data[:post_code] %>' +
  '<%= "<p><strong>Telefon:</strong> #{@place_info.data[:phone_number]}</p>".html_safe if @place_info.data[:phone_number] %>'
)
